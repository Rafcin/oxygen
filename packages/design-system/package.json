  "name": "@oxygen/design-system",
  "description": "Design System by Rafcin",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/oxygen.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "design system",
    "mui"
  ],
  "main": "./dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./internal": {
      "import": "./dist/esm/internal.js",
      "require": "./dist/internal.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "[--- Build ---]": "",
    "build": "rollup --config",
    "build:all": "pnpm js:theme && pnpm dts:theme && pnpm build",
    "[--- Dev ---]": "",
    "dev": "tsup --watch",
    "dev:build": "tsup",
    "[--- Lint ---]": "",
    "lint": "tsc --noEmit --project tsconfig.dev.json && pnpm eslint ./src --ext .js,.ts,.tsx",
    "[--- Clean ---]": "",
    "clean": "rimraf dist node_modules",
    "[--- Storybook ---]": "",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --docs",
    "storybook:preview": "serve storybook-static",
    "[--- Test ---]": "",
    "test": "pnpm test:unit",
    "test:watch": "pnpm test:unit:watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "[--- Utils ---]": "",
    "ts": "node -r esbuild-register",
    "size": "pnpm run size-limit",
    "generate:theme": "pnpm js:theme && pnpm dts:theme",
    "js:theme": "pnpm jtj -i ./src/core/theme/theme.json -o ./src/core/theme/theme.ts --ts -n theme",
    "dts:theme": "quicktype --top-level Theme -l typescript ./src/core/theme/theme.json -o ./src/declarations/theme/index.ts && ./scripts/prefix.sh ./src/declarations/theme/index.ts Oxygen"
  },
