    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-is": "18.2.0",
    "react-use": "^17.4.0",
    "tslib": "2.4.1",
    "use-places-autocomplete": "^4.0.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11",
    "zustand": "^4.3.2"
  },
