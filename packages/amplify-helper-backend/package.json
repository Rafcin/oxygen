{
  "private": false,
  "license": "Apache-2.0",
  "version": "0.0.0",
  "name": "@oxygen/amplify-helper-backend",
  "description": "TRPC backend for Amplify Helper",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/oxygen.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "amplify",
    "helper",
    "TRPC",
    "trpc"
  ],
  "main": "./dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "[--- Build ---]": "",
    "build": "pnpm prisma-trpc && rollup --config",
    "prisma-trpc": "prisma generate",
    "[--- Dev ---]": "",
    "dev": "tsup --watch",
    "dev:build": "tsup",
    "ts-watch": "tsc --project tsconfig.watch.json",
    "[--- Lint ---]": "",
    "lint": "tsc --noEmit --project tsconfig.dev.json && pnpm eslint ./src --ext .js,.ts,.tsx",
    "[--- Clean ---]": "",
    "clean": "rimraf dist node_modules",
    "[--- Utils ---]": "",
    "ts": "node -r esbuild-register",
    "size": "pnpm run size-limit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.9.0",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "next-auth": "^4.18.10",
    "prisma": "^4.9.0",
    "prisma-trpc-generator": "^1.0.2",
    "superjson": "1.12.2",
    "trpc-shield": "^0.4.0",
    "zod": "^3.20.2",
    "zod-prisma-types": "^1.6.8"
  },
  "devDependencies": {
    "@oxygen/tsconfig": "*",
    "@prisma/client": "^4.9.0",
    "@trpc/server": "^10.9.0",
    "eslint-config-custom-oxygen": "*",
    "prisma": "^4.9.0"
  }
}
