{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["build"]
    },
    "tsc": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "storybook": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    },
    "update-dependencies": {
      "cache": false
    },
    "prisma:generate": {
      "cache": false
    },
    "prisma:push": {
      "cache": false
    }
  }
}
