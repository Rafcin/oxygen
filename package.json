{
  "name": "oxygen",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "[----[Build]----]": "",
    "build": "turbo run build",
    "[----[Dev]----]": "",
    "dev": "turbo run dev",
    "[----[Lint/Format]----]": "",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "[----[Clean]----]": "",
    "clean": "turbo run clean && rimraf node_modules pnpm-lock.yaml && pnpm i"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-next": "13.1.6",
    "is-ci": "^3.0.1",
    "prettier": "latest",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.2",
    "tsup": "^6.6.3",
    "turbo": "latest",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@tsconfig/recommended": "^1.0.2"
  },
  "packageManager": "pnpm@7.27.1"
}
